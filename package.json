{
  "name": "vault-garden",
  "version": "0.1.0",
  "description": "Vault.Garden website",
  "author": "Perry Mitchell",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "run-s clean build:web",
    "build:web": "webpack --config webpack.config.cjs --mode=production",
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run build -- -w\" \"npm run serve\"",
    "serve": "sleep 1 && serve -l 5000 --no-port-switching ./dist"
  },
  "private": true,
  "devDependencies": {
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "pug-plugin": "^5.3.0",
    "rimraf": "^6.0.1",
    "sass": "^1.77.4",
    "sass-loader": "^14.2.1",
    "serve": "^14.2.3",
    "ts-loader": "^9.5.1",
    "typescript": "5.3.3",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  }
}
